---
title: 'People'
layout: 'default'
isPage: true
---
<nav class="sidebar">
  <ul class="menu">
    {{#each peopleProfiles}}
    <a href="#{{ parameterize title }}"><li>{{title}}</li></a>
    {{/each}}
  </ul>
</nav>
<section class="content">
  {{#each peopleProfiles }}
  <section id="{{ parameterize title }}" class="card profile">
      <div class="profile-picture">
        <img src="{{ profile_picture }}"></img>
      </div>
      <header>
        <h1>{{title}}</h1>
      </header>
      <section>
        {{{ contentRenderedWithoutLayouts }}}
      </section>
    </section>
  {{/each}}
</section>
