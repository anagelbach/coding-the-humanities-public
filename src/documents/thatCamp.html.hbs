---
title: "THATCamp"
layout: 'presentation'
isPresentation: true
---

{{#each presentations.thatCamp.slides}}
<section class="card step slide {{type}} {{#if section}}section{{section}}{{/if}}" id="{{parameterize title}}" data-x="{{position @index}}">
  <header>
    <h1>{{title}}</h1>
    {{#if subtitle}}
    <h2>{{subtitle}}</h2>
    {{/if}}
    {{#if section-title}}
    <h2>{{section-title}}</h2>
    {{/if}}
  </header>
  <section>
    {{{ contentRenderedWithoutLayouts }}}
  </section>
</section>
{{/each}}
